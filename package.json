{
  "name": "s3-deploy-action",
  "version": "1.1.0",
  "private": true,
  "description": "deploy SPA to AWS S3",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "package": "npm-run-all package:*",
    "package:main": "ncc build -o dist --source-map --license licenses.txt",
    "package:deploy-path": "ncc build src/deploy-path.ts -o deploy-path/dist --source-map --license licenses.txt",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "all": "npm run lint && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "glob": "^8.1.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^5.46.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.29.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^26.6.3",
    "jest-circus": "^27.3.1",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.4"
  }
}
