{
  "name": "s3-deploy-action",
  "version": "1.7.0",
  "private": true,
  "description": "deploy SPA to AWS S3",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "package": "npm-run-all package:*",
    "package:main": "ncc build -o dist --source-map --license licenses.txt",
    "package:deploy-path": "ncc build src/deploy-path.ts -o deploy-path/dist --source-map --license licenses.txt",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "all": "npm run lint && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "glob": "^11.0.1"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.22.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.22.0",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
